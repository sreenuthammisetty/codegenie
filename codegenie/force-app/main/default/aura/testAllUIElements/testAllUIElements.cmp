            <input type="text" id="text-input-id-1" class="slds-input" placeholder="Placeholder Text" />
        </div>
    </div>
     <br />
    <div class="slds-form-element">
    	<div class="slds-form-element__control">
            <span class="slds-checkbox">
            	<input type="checkbox" name="options" id="checkbox-2" value="on" />
                <label class="slds-checkbox__label" for="checkbox-2">
                    <span class="slds-checkbox_faux"></span>
                    <span class="slds-form-element__label">Willing to Travel</span>
                </label>
            </span>
    	</div>
    </div>
     <br />
    <div class="slds-checkbox_add-button">
    	<input type="checkbox" class="slds-assistive-text" id="add-checkbox-2" value="on" />
        <label for="add-checkbox-2" class="slds-checkbox_faux">
        	<span class="slds-assistive-text">Add product</span>
        </label>
    </div>
    
    <br />
	
    <div class="demo-only slds-size_1-of-2">
        <div class="slds-form-element">
